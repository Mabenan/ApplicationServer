{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "(msvc) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/../ApplicationServer-build/debug/ApplicationServer/ApplicationServer.exe",
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/../ApplicationServer-build/debug/ApplicationServer/",
        "externalConsole": true,
        "MIMode": "gdb",
        "preLaunchTask": "cpLibs",
        "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
          "logging": { "engineLogging": true },
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ]
      }
    ]
  }